  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "globby": "^11.0.1",
    "markdown2html-pro": "^1.0.6",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "remark": "^12.0.0",
    "remark-external-links": "^6.1.0",
